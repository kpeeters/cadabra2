def test18():
    __cdbkernel__=create_scope()
    {\chi, \psi}::Weight(label=grassmann, value=1);
    ex:= \chi \psi \chi;
    drop_weight(_, $grassmann=2$)
    tst1:= \chi\psi\chi - @(ex);
    assert(tst1==0)
    drop_weight(ex, $grassmann=3$)
    assert(ex==0)
    print("Test 18 passed")
        
test18()

