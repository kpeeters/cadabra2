def test04():
    __cdbkernel__=create_scope()
    R_{a b c d}::TableauSymmetry(shape={2,2}, indices={0,2,1,3}).
    obj:= R_{a b c d} R_{d c a b}:
    canonicalise(_)
    tst:= (-1) R_{a b c d} R_{a b c d} - @(obj);
    collect_terms(_)
    assert(tst==0)
    print('Test 04 passed')

test04()

