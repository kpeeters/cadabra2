def test08():
    { m, n, p, q }::Indices(vector).
    { A_{m n p}, B_{m n p} }::AntiSymmetric.
    { m, n, p, q }::Integer(1..3).
    obj:= A_{m n p} B_{m n q} - A_{m n q} B_{m n p};
    decompose_product(_)
    canonicalise(_)
    assert(obj==0)
    print('Test 08 passed')

test08()

