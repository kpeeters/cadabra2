# All examples from the hep-th paper, converted to version 2 format.

def test01():
    __cdbkernel__=create_scope()
    { a, b, c, d }::Indices.
    obj:= A_{a b} B_{b c};
    substitute(_, $B_{a b} ->  C_{a b c} D_{c}$ )
    tst:= A_{a b}  C_{b c d} D_{d} - @(obj);
    assert(tst==0)
    print('Test 01 passed')

test01()

