def test02():
    __cdbkernel__=create_scope()
    A_{\dot{a} \dot{b}}::AntiSymmetric.
    obj:= A_{\dot{b} \dot{a}}:
    canonicalise(_)
    tst:= A_{\dot{a} \dot{b}} + @(obj);
    assert(tst==0)
    print('Test 02 passed')

test02()

