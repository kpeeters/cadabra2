def test03():
    __cdbkernel__=create_scope()
    { a_{1}, a_{2}, a_{3}, a_{4} }::Indices(vector).
    obj:= V_{a_{1}} W_{a_{1}}:
    substitute(_, $V_{a_{2}} -> M_{a_{2} a_{1}} N_{a_{1}}$ )
    tst:= M_{a_{1} a_{2}}  N_{a_{2}}  W_{a_{1}} - @(obj);
    collect_terms(_)
    assert(tst==0)
    print('Test 03 passed')

test03()

